{
  "name": "trpc-figma",
  "version": "0.1.0-alpha.1",
  "description": "tRPC adapter for figma plugins",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build:pkg": "tsup src/index.ts --format esm,cjs --dts",
    "prebuild": "rimraf dist",
    "build": "concurrently pnpm:typcheck pnpm:build:pkg"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@trpc/client": "^10.0.0",
    "@trpc/server": "^10.0.0"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.57.0",
    "@types/node": "^18.11.11",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "tslib": "^2.4.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  }
}
