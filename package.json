{
  "name": "trpc-figma",
  "version": "0.1.3",
  "description": "tRPC adapter for figma plugins",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsup src/index.ts --watch",
    "dev:all": "pnpm run dev & pnpm run -r dev",
    "typecheck": "tsc --noEmit",
    "build:pkg": "tsup src/index.ts --format cjs --dts",
    "prebuild": "rimraf dist",
    "build": "pnpm run typecheck && pnpm run build:pkg"
  },
  "keywords": [],
  "author": "labXD",
  "contributors": [
    "Walee Ahmed <walee@labxd.com> (https://github.com/wallexd)",
    "Jimmy Nguyen <jimmy@labxd.com> (https://github.com/jimmynxd)"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/labXD/figma-trpc/"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@trpc/client": "^10.5.0",
    "@trpc/server": "^10.5.0"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.57.0",
    "@swc/core": "^1.3.23",
    "@trpc/client": "^10.5.0",
    "@trpc/server": "^10.5.0",
    "@types/node": "^18.11.11",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "tslib": "^2.4.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  }
}
